{
	"name": "cdk-example",
	"version": "1.0.0",
	"bin": {
		"cdk-example": "build/infra/index.js"
	},
	"scripts": {
		"build": "rm -rf build && tsc -p tsconfig.build.json",
		"watch": "tsc -w",
		"test": "NO_LOG=true jest --silent --runInBand",
		"test:v": "jest --runInBand",
		"test:vv": "jest --verbose --runInBand",
		"test:w": "jest --watchAll --runInBand",
		"cdk": "cdk",
		"lint": "eslint . --ext .ts --fix",
		"format": "prettier --write --check 'src/**/*.ts' 'src/**/*.test.ts' 'infra/**/*.ts'",
		"check": "tsc",
		"synth": "cdk synth",
		"ci": "npm run test && npm run format && npm run lint",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@aws-cdk/assert": "^2.31.1",
		"@aws-sdk/client-sqs": "^3.121.0",
		"@types/aws-lambda": "^8.10.101",
		"@types/jest": "^28.1.4",
		"@types/node": "18.0.3",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1171.0",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"jest": "28.1.2",
		"prettier": "^2.7.1",
		"ts-jest": "^28.0.5",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.8.2",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@aws-cdk/aws-apigateway": "^1.163.1",
		"@aws-cdk/aws-cognito": "^1.163.1",
		"@aws-cdk/aws-iam": "^1.163.1",
		"@aws-cdk/aws-lambda": "^1.163.1",
		"@aws-cdk/core": "^1.163.1",
		"aws-cdk": "^2.31.1",
		"aws-xray-sdk-core": "^3.3.6",
		"source-map-support": "^0.5.21"
	},
	"private": true,
	"eslintConfig": {
		"env": {
			"node": true,
			"jest": true,
			"es2020": true
		},
		"parser": "@typescript-eslint/parser",
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended",
			"plugin:prettier/recommended"
		],
		"parserOptions": {
			"sourceType": "module"
		},
		"rules": {
			"no-console": ["error"],
			"no-debugger": ["error"],
			"linebreak-style": ["error", "unix"],
			"semi": ["error", "always"],
			"no-unused-vars": "off",
			"@typescript-eslint/no-unused-vars": ["error"],
			"no-prototype-builtins": "off"
		}
	},
	"engines": {
		"node": ">=16"
	}
}
